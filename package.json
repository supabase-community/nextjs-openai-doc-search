    "unist-util-filter": "^4.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/estree": "^1.0.0",
    "@types/mdast": "^3.0.11",
    "@types/yargs": "^17.0.24",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.0.3",
